@media print {
  body {
    -webkit-print-color-adjust: exact;
  }
  .checkBox {
    -webkit-print-color-adjust: exact;
  }
}
html,
body {
  background-color: #fff;
  margin: 0px;
}
.page {
  position: relative;
  height: 246mm;
  width: 217mm;
  font-family: 'Verdana';
  display: block;
  background: #fff;
}
.row {
  height: 5mm;
  width: 217mm;
}
.heading {
  position: relative;
  height: 5mm;
  width: 217mm;
  background: #a95346;
  font-weight: bold;
  text-align: center;
  letter-spacing: 0.1mm;
  color: #a95346;
}
.whiteBox {
  min-height: 5mm;
  height: auto;
  font-size: 3mm;
  line-height: 2;
  border-left: 1px solid #a95346;
  border-bottom: 1px solid #a95346;
  color: #000;
  letter-spacing: 0.1mm;
  float: left;
  display: inline-block;
  text-align: center;
}
.brownBox {
  height: 5mm;
  font-size: 3mm;
  line-height: 2;
  border-left: 1px solid #a95346;
  border-bottom: 1px solid #a95346;
  color: #a95346;
  letter-spacing: 0.1mm;
  float: left;
  display: inline-block;
  text-align: center;
}
.yellowBox {
  height: 5mm;
  font-size: 3mm;
  line-height: 2;
  border-left: 1px solid #a95346;
  border-bottom: 1px solid #a95346;
  color: #a95346;
  letter-spacing: 0.1mm;
  float: left;
  display: inline-block;
  text-align: center;
  background-color: #fff19c;
}
.checkBox {
  width: 2mm;
  height: 2mm;
  background-color: #fff;
  position: relative;
  display: inline-block;
  box-shadow: 2px 2px #a95346;
}
.underLine {
  height: 4mm;
  width: 68mm;
  position: relative;
  display: inline-block;
  float: left;
}
.highlight {
  background-color: #fff19c;
}
